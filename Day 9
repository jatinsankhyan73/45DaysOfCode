#include <stdio.h>

int main() {
	int t,x,y,m;
 scanf("%d",&t);
 while(t--){
      scanf("%d %d ",&x,&y);
      m=y/x;
      if(x<y){
          if(y%x==0) { 
          
              printf("%d\n",(y/x)-1);
      
          }
          else 
              printf("%d\n",y/x);
      }
      else{
          printf("0\n");
      }
 }
	return 0;
}
