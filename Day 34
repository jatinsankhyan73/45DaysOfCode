#include <stdio.h>

int main(void) {
	int t;
	scanf("%d",&t);
	while(t--){
	    int a,b,x,y;
	    scanf("%d %d %d %d",&a,&b,&x,&y);
	    if(a>b){
	        if(y>=(a-b))
	            printf("yes\n");
	        else 
	        printf("no\n");
	    }
	    else if(a<b){
	        if(x>=(b-a))
	            printf("yes\n");
	        else 
	            printf("no\n");
	    }
	    else if(a==b)
	        printf("yes\n");
	}
	return 0;
}
