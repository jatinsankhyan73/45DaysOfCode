#include <stdio.h>

int main(void) {
	int t;
	scanf("%d",&t);
	while(t--){
	    int w,x,y,z;
	    scanf("%d %d %d %d ",&w,&x,&y,&z);
	    if(x+y==w || x==w || y==w || w==x+y+z)
	        printf("YES\n");
	    else if(z+y==w|| z==w || y==w)
	        printf("YES\n");
	    else if(x+z==w || x==w || z==w)
	        printf("YES\n");     
	    else
	        printf("NO\n");      
	}
	return 0;
}
