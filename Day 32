#include <stdio.h>
#include <stdlib.h>

int main(void) {
	int t,n,x,i;
	scanf("%d",&t);
	while(t--){
	    scanf("%d %d ",&n,&x);
	    int a[n] , count = 0;
	    for(i=0;i<n;i++)
	    {
	        scanf("%d",&a[i]);
	   
	    }
	    for(int i = 0; i<n;i++){
	        if(a[i]>=x){
	            count++;
	        }
	    }
	    printf("%d\n",count);
	}
	return 0;
}


