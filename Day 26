#include <stdio.h>

int main(void) {
    int t,a,b,c,x;
    scanf("%d",&t);
    while(t--){
        scanf("%d %d %d %d ",&a,&b,&c,&x);
        if (a>=c && b>=c)
        (a+b>=x)?printf("YES\n"):printf("NO\n");
        else if (b>=a && c>=a)
        (b+c>=x)?printf("YES\n"):printf("NO\n");
        else if (c>=b && a>=b)
        (a+c>=x)?printf("YES\n"):printf("NO\n");
    }
	return 0;
	
}

