#include <stdio.h>

int main(void) {
    int t,a1,a2,b1,b2,c;
    scanf("%d",&t);
    while(t--){
        scanf("%d%d%d%d",&a1,&a2,&b1,&b2);
        c=(a2-a1)+(b2-b1);
        if(c<=0)
        {
            printf("NO\n");
        }
        else if(c>0)
        {
            printf("YES\n");
        }
    }
    return 0;
}

